extends layout

block content
    if !migStatus
        .item
            .headliner Migration Wizard
        .item
            .subliner The easy way to migrate your MySQL database into MongoDB

        .item
            a(href='/migrate')
                button Lets start
                
    else
        .item
            p Migration complete
            table
                each user in userList
                    tr
                    td #{user.userName}
                    td #{user.email}
                    td #{user.regDate}